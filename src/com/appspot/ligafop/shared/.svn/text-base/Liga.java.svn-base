package com.appspot.ligafop.shared;

import java.util.ArrayList;

import com.google.gwt.user.client.rpc.IsSerializable;

public class Liga implements IsSerializable {

	private ArrayList<JogadorLiga> jogadores;

	private BoardGame boardGame;

	private Jogador jogador;
	
	private int partidas;
	
	private Double pontuacaoMaxima;
	
	public Liga() {
		pontuacaoMaxima = 0d;
	}

	public Liga(BoardGame boardGame) {
		this.boardGame = boardGame;
	}

	public ArrayList<JogadorLiga> getJogadores() {
		return jogadores;
	}

	public void setJogadores(ArrayList<JogadorLiga> jogadores) {
		this.jogadores = jogadores;
	}

	public BoardGame getBoardGame() {
		return boardGame;
	}

	public void setBoardGame(BoardGame boardGame) {
		this.boardGame = boardGame;
	}

	public Jogador getJogador() {
		return jogador;
	}

	public void setJogador(Jogador jogador) {
		this.jogador = jogador;
	}

	public int getPartidas() {
		return partidas;
	}

	public void setPartidas(int partidas) {
		this.partidas = partidas;
	}

	public Double getPontuacaoMaxima() {
		return pontuacaoMaxima;
	}

	public void setPontuacaoMaxima(Double pontuacaoMaxima) {
		this.pontuacaoMaxima = pontuacaoMaxima;
	}


}
